#!/usr/bin/env bash

# Loop through each file in the current directory
for file in *; do
    # Check if the file is a regular file (not a directory) and not hidden
    if [ -f "$file" ] && [[ "$file" != .* ]]; then
        # Extract the part of the name after the first dash using cut
        name=$(echo "$file" | cut -d '-' -f 2-)
        echo "$name"
    fi
done
